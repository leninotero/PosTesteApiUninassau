{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "nome",
    "preco",
    "descricao",
    "quantidade",
    "_id"
  ],
  "properties": {
    "nome": {
      "type": "string",
      "default": "",
      "title": "The nome Schema",
      "examples": [
        "Base de Computador"
      ]
    },
    "preco": {
      "type": "integer",
      "default": 0,
      "title": "The preco Schema",
      "examples": [
        200
      ]
    },
    "descricao": {
      "type": "string",
      "default": "",
      "title": "The descricao Schema",
      "examples": [
        "Base para refrigeração de notebook"
      ]
    },
    "quantidade": {
      "type": "integer",
      "default": 0,
      "title": "The quantidade Schema",
      "examples": [
        50
      ]
    },
    "_id": {
      "type": "string",
      "default": "",
      "title": "The _id Schema",
      "examples": [
        "BeeJh5lz3k6kSIzA"
      ]
    }
  },
  "examples": [{
    "nome": "Base de Computador",
    "preco": 200,
    "descricao": "Base para refrigeração de notebook",
    "quantidade": 50,
    "_id": "BeeJh5lz3k6kSIzA"
  }]
}